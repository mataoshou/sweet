package tool.common;

import java.util.UUID;

public class GuidUtils
{
	/**
	 *获取普通的guid 
	 */
	public static String guid()
	{
		String id=UUID.randomUUID().toString();
		return id;
	}
	/**
	 *获取不含-的guid 
	 */
	public static String guid2()
	{
		String id=guid();
		id=id.replace("-", "");
		return id;
	}
}
